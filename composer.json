{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.2",
        "ext-gd": "*",
        "barryvdh/laravel-dompdf": "^3.0",
        "barryvdh/laravel-snappy": "^1.0.0",
        "doctrine/dbal": "^3",
        "eluceo/ical": "^0.13.0",
        "fedeisas/laravel-mail-css-inliner": "^5.0",
        "google/apiclient": "^2.0",
        "guzzlehttp/guzzle": "^7.4",
        "igomis/laravel-html": "*",
        "laravel/framework": "^12.0",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.4.1",
        "laravel/ui": "^4.0",
        "lcobucci/jwt": "^5.3",
        "livewire/livewire": "3.7.3",
        "maatwebsite/excel": "*",
        "mikehaertl/php-pdftk": "^0.8.1",
        "php-webdriver/webdriver": "^1.13",
        "phpoffice/phpspreadsheet": "^1.26",
        "predis/predis": "^1.1",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.6",
        "simplesoftwareio/simple-qrcode": "~4",
        "smalot/pdfparser": "^2.1",
        "symfony/http-client": "^7.0",
        "symfony/mailgun-mailer": "^7.0",
        "symfony/process": "^7.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "fakerphp/faker": "*",
        "filp/whoops": "^2.0",
        "laravel/browser-kit-testing": "^7.0",
        "laravel/sail": "^1.21",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^8.0@beta",
        "phpunit/phpunit": "^11.0",
        "symfony/css-selector": "^6.2",
        "symfony/dom-crawler": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Intranet\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "packages/html"
        },
        {
            "type": "path",
            "url": "packages/laravelcollective/html"
        }
    ],
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "test:focus": [
            "@php -d max_execution_time=300 artisan test --stop-on-failure --filter=Expediente"
        ],
        "test:quick": [
            "@php -d max_execution_time=300 artisan test --stop-on-failure --filter=Expediente|Empresa|Comision"
        ],
        "test:full": [
            "@php -d max_execution_time=1000 artisan test --stop-on-failure"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.3.1"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
