{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3|^8.0",
        "barryvdh/laravel-dompdf": "^0.8.6",
        "barryvdh/laravel-snappy": "^0.4.7",
        "doctrine/dbal": "^2.6",
        "eluceo/ical": "^0.13.0",
        "fedeisas/laravel-mail-css-inliner": "^4.0",
        "fideloper/proxy": "^4.0",
        "graham-campbell/markdown": "^13.1.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "h4cc/wkhtmltopdf-amd64": "^0.12.4",
        "izi-dev/nova-conditional-field": "^2.1",
        "jenssegers/date": "^4.0.0@beta",
        "laravel/framework": "^8.0",
        "laravel/nova": "*",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.4.1",
        "laravel/ui": "^2.0",
        "laravelcollective/html": "^6.0",
        "mikehaertl/php-pdftk": "^0.8.1",
        "predis/predis": "^1.1",
        "simplesoftwareio/simple-qrcode": "~4",
        "styde/html": "~1.9"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "composer/composer": "^1.9",
        "filp/whoops": "^2.0",
        "laravel/browser-kit-testing": "^6.2",
        "laravel/dusk": "^6.13",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0",
        "symfony/css-selector": "^5.0",
        "symfony/dom-crawler": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Intranet\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        }
    ],
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
